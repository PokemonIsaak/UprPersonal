<Page x:Class="RomanovEPractice3.Pages.PrintServicesPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RomanovEPractice3.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Страница отчёта">

    <Grid>
        <WebBrowser Margin="10 10 10 40" x:Name="BrowserReport"></WebBrowser>
        <Button Height="30" Width="200" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="5 5 10 5" x:Name="BtnPrint" Click="BtnPrint_Click">Печать</Button>
        <Button Height="30" Width="200" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="5 5 220 5" x:Name="BtnPrintPDF" Click="BtnPrintPDF_Click">Печать в pdf</Button>
    </Grid>
</Page>
